<Window x:Class="Sklad.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sklad"
        mc:Ignorable="d"
        Title="ИС Склад" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="360*"/>
            <RowDefinition Height="40*"/>
        </Grid.RowDefinitions>
        <Grid Background="#f7ca57"></Grid>
        <TextBlock Text="warehouse" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="25" FontFamily="Sylfaen"></TextBlock>
        <Image Source="Resources/logo.png" HorizontalAlignment="Left"></Image>
        <Button Name="BtnBack" Click="BtnBack_Click" HorizontalAlignment="Right" Content="Назад" Margin="2"></Button>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Grid Grid.Row="2" Background="#b3b3b3"></Grid>
    </Grid>
</Window>
